<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net461;net5.0;net6.0;net7.0</TargetFrameworks>
    <Version>1.4.5</Version>
    <LangVersion>9.0</LangVersion>
    <Authors>https://github.com/asvol</Authors>
    <Company>https://github.com/asvol</Company>
    <Copyright>https://github.com/asvol</Copyright>
    <PackageProjectUrl>https://github.com/asvol/asv-common</PackageProjectUrl>
    <RepositoryUrl>https://github.com/asvol/asv-common</RepositoryUrl>
    <PackageTags />
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Provides simple file store</Description>
    <PackageIcon>asv-store-128.png</PackageIcon>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\img\asv-store-128.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="DynamicData" Version="7.12.1" />
    <PackageReference Include="LiteDB" Version="5.0.12" />
    <PackageReference Include="NLog" Version="5.0.5" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Asv.Common\Asv.Common.csproj" />
  </ItemGroup>
</Project>